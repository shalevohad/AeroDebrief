             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:server="clr-namespace:Ciribob.DCS.SimpleRadio.Standalone.Server">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <server:Bootstrapper x:Key="Bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>